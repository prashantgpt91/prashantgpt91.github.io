[{% assign posts = '' | split: '/' %}{% for post in site.posts %}{% if post.queued == null %}{% assign posts = posts | push: post %}{% endif %}{% endfor %}{% for post in posts %}{"title": "{{ post.title | escape }}","tags": "{{ post.tags | join: ', ' }}","url": "{{ site.baseurl }}{{ post.url }}","date": "{% include helpers/format-date.html date=post.date %}"}{% unless forloop.last %},{% endunless %}{% endfor %}]